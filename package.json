{
  "name": "joyce-rest",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run rest\" \"npm run mesh\"",
    "rest": "ENABLED_COLLECTIONS=plito,senio nodemon --watch './src/**/*.ts' --exec ts-node ./src/index.ts",
    "mesh": "node src/db/graphql/meshrc.js && node src/db/graphql/dl.js && mesh serve",
    "build": "tsc",
    "test": "jest --coverage",
    "test:watch": "npm run test -- --watchAll",
    "commit": "git cz",
    "start": "concurrently --kill-others \"npm run mesh\" \"npm run start-dist\"",
    "start-dist": "node dist/index.js",
    "package-upgrade": "npx npm-check-updates -u && npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-mesh/cli": "0.30.2",
    "@graphql-mesh/mongoose": "0.7.15",
    "@vercel/fetch": "^6.1.1",
    "dotenv": "^10.0.0",
    "fastify": "^3.19.1",
    "fastify-cors": "^6.0.2",
    "fastify-http-proxy": "^6.0.0",
    "fastify-oas": "^3.0.8",
    "graphql": "15.5.1",
    "js-yaml": "^4.1.0",
    "kafka-node": "^5.0.0",
    "mongodb": "^3.0.0",
    "mongoose": "^5.13.3",
    "node-fetch": "^2.6.1",
    "pino": "^6.12.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.1.15",
    "@types/cors": "^2.8.12",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.2",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "git-cz": "^4.7.6",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
